TYPEMAP
FCGI		T_PTROBJ
FCGI::Stream	T_PTROBJ
GLOBREF		T_GLOBREF
HASHREF		T_HASHREF

INPUT
T_GLOBREF
    if (SvROK($arg) && isGV(SvRV($arg))) {
	$var = (GV*)SvRV($arg);
    } else
	croak(\"$var is not a GLOB reference\");
T_HASHREF
    if (SvROK($arg) && SvTYPE(SvRV($arg)) == SVt_PVHV) {
	$var = (HV*)SvRV($arg);
    } else
	croak(\"$var is not a reference to a hash\");
